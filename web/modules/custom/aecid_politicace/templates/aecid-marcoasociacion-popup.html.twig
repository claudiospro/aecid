<div class="aecid-intranet-lightbox-content-body">
  <table width="100%">
    <tbody>
    {% if data['items'] %}
    {% if data['items']|length > 0 %}
    {% for item in data['items'] %}
      <tr>
        <td>{{ item['name'] }}</td>
        <td><a href="/taxonomy/term/{{ item['tid'] }}/edit?destination=/intranet/marcoasociacion/listado"><i class="fa fa-pencil" aria-hidden="true"></i></a></td>
        <td><a href="/taxonomy/term/{{ item['tid'] }}/delete?destination=/intranet/marcoasociacion/listado"><i class="fa fa-trash" aria-hidden="true"></i></a></td>
      </tr>
    {% endfor %}
    {% endif %}
    {% endif %}
    </tbody>
  </table>
</div>
